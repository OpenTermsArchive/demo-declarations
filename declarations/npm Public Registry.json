{
  "name": "npm Public Registry",
  "terms": {
    "Privacy Policy": {
      "fetch": "https://docs.npmjs.com/policies/privacy",
      "select": {
        "startBefore": "h1",
        "endBefore": "#toc-heading"
      },
      "remove": "details"
    },
    "Terms of Service": {
      "fetch": "https://docs.npmjs.com/policies/open-source-terms",
      "select": [
        "main"
      ],
      "remove": [
        "nav",
        "details"
      ]
    },
    "Copyright Claims Policy": {
      "fetch": "https://docs.npmjs.com/policies/dmca",
      "select": [
        "main"
      ],
      "remove": [
        "nav",
        "details"
      ]
    }
  }
}
